{
  "name": "e-commerce-bootcamp-frontend",
  "version": "1.0.0",
  "description": "E-Commerce bootcamp store frontend",
  "license": "Lukasz Wyspianski",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config tools/webpack/webpack.config.dev.js & node dev-server/server.js",
    "start-keycloak": "docker-compose -f frontend-dev-docker-compose.yaml up",
    "start-no-backend": "cross-env NODE_ENV=development webpack serve --config tools/webpack/webpack.config.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.prod.js",
    "test": "jest",
    "serve": "cross-env NODE_ENV=production webpack --config tools/webpack/webpack.config.prod.js && cross-env serve dist/",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "server": "node dev-server/server.js"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jquery": "^3.5.16",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@types/react-test-renderer": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "babel-jest": "^29.3.1",
    "clsx": "^2.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jsonwebtoken": "^9.0.2",
    "jwk-to-pem": "^2.0.5",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^12.0.0",
    "less": "^4.1.3",
    "less-loader": "11.1.0",
    "mini-css-extract-plugin": "^2.6.1",
    "msw": "^1.0.0",
    "prettier": "^2.8.3",
    "react-refresh": "^0.14.0",
    "react-test-renderer": "^18.2.0",
    "react-tooltip": "^5.21.5",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "serve": "^14.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "9.4.1",
    "typescript": "^4.8.4",
    "uuid": "^9.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^1.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-owl-carousel": "^2.3.3",
    "react-redux": "^8.1.1",
    "react-router-dom": "6",
    "react-toastify": "^9.1.3"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "moduleNameMapper": {
      "^@src(.*)$": "<rootDir>/src$1",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/setup-jest.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
